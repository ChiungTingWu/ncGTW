unlockBinding("strucplot", as.environment("package:vcd")) 
assign("strucplot", my.strucplot, "package:vcd") 


unlockBinding(name, env);
assignInNamespace(name, value, ns=pkgName, envir=env); 
assign(name, value, envir=env); 
lockBinding(name, env); 


unlockBinding("getPeaks", as.environment("package:xcms"))
assignInNamespace("getPeaks", getPeaks, ns="xcms", envir = as.environment("package:xcms"))
assign("getPeaks", getPeaks, envir=as.environment("package:xcms"))
lockBinding("getPeaks", as.environment("package:xcms"))

unlockBinding("fillPeaksChromPar", as.environment("package:xcms"))
assignInNamespace("fillPeaksChromPar", fillPeaksChromPar, ns="xcms", envir = as.environment("package:xcms"))
assign("fillPeaksChromPar", fillPeaksChromPar, envir=as.environment("package:xcms"))
lockBinding("fillPeaksChromPar", as.environment("package:xcms"))


lcra <- xcmsRaw(filePaths[1])
lcra@profparam <- list(1,2)
